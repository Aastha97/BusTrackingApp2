<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffffff">


        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp"
            android:text="Seat Booking"
            android:textColor="@android:color/holo_blue_dark"
            android:textSize="35sp"
            android:textStyle="bold" />


        <Button
            android:id="@+id/button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView21"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="37dp"
            android:layout_marginEnd="54dp"
            android:text="BOOK" />

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView21"
            android:layout_marginTop="38dp"
            app:srcCompat="@drawable/seat_unbooked" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/imageButton"
            android:layout_toStartOf="@+id/button2"
            app:srcCompat="@drawable/seat_unbooked" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView21"
            android:layout_alignTop="@+id/imageButton2"
            app:srcCompat="@drawable/seat_unbooked" />

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageButton3"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="11dp"
            app:srcCompat="@drawable/seat_unbooked" />

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/imageButton"
            android:layout_marginTop="32dp"
            app:srcCompat="@drawable/seat_unbooked" />

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/seat_unbooked"
            android:layout_alignTop="@+id/imageButton5"
            android:layout_alignStart="@+id/imageButton2" />

        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/seat_unbooked"
            android:layout_alignTop="@+id/imageButton6"
            android:layout_toStartOf="@+id/imageButton4" />

        <ImageButton
            android:id="@+id/imageButton8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/imageButton4"
            android:layout_alignTop="@+id/imageButton7"
            app:srcCompat="@drawable/seat_unbooked" />

        <ImageButton
            android:id="@+id/imageButton9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="37dp"
            app:srcCompat="@drawable/seat_unbooked"
            android:layout_below="@+id/imageButton5"
            android:layout_toStartOf="@+id/imageButton10" />

        <ImageButton
            android:id="@+id/imageButton10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageButton9"
            android:layout_alignEnd="@+id/imageButton6"
            app:srcCompat="@drawable/seat_unbooked" />

        <ImageButton
            android:id="@+id/imageButton11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageButton10"
            android:layout_alignStart="@+id/imageButton3"
            app:srcCompat="@drawable/seat_unbooked" />

        <ImageButton
            android:id="@+id/imageButton12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageButton11"
            android:layout_alignEnd="@+id/imageButton8"
            app:srcCompat="@drawable/seat_unbooked" />


    </RelativeLayout>
</LinearLayout>
